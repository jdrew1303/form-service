{
  "name": "form-service",
  "version": "2.0.17",
  "description": "form-service",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "coverage": "nyc npm test",
    "build": "env NODE_ENV=production webpack --config webpack.config.prod.js -p",
    "start": "webpack-dev-server --config webpack.config.dev.js --port 3000 --history-api-fallback",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "mocha:json": "mocha --reporter=json > coverage/mocha.json",
    "test": "NODE_ENV=test mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "keywords": [
    "zooid",
    "zooid-app",
    "react",
    "babel",
    "webpack"
  ],
  "homepage": "https://github.com/octoblu/form-service",
  "bugs": "https://github.com/octoblu/form-service/issues",
  "author": {
    "name": "Octoblu Inc",
    "url": "https://github.com/octoblu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octoblu/form-service.git"
  },
  "license": "MIT",
