    "autoprefixer": "^6.3.6",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
