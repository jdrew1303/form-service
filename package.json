    "gzip-size": "^3.0.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.8.0",
    "json-schema-defaults": "^0.3.0",
    "lodash": "^4.17.4",
    "postcss-loader": "^1.3.3",
